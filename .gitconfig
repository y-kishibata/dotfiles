[include]
  path = .gitconfig.local
[core]
  excludesfile = /Users/kishi/.gitignore_global
  editor = vim -c \"set fenc=utf-8\"
[difftool "sourcetree"]
  cmd = opendiff \"$LOCAL\" \"$REMOTE\"
  path = 
[mergetool "sourcetree"]
  cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
  trustExitCode = true
[color]
	diff = auto
	status = auto
	branch = auto
	ui = auto

[alias]
  logf = log --name-status --pretty=format:\"%C(red)%h %C(green)%an %Creset%s %C(yellow)%d%Creset\"
  logg = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
  s = status
  st = status
  ch = checkout
  f = fetch
  fp = fetch -p
  p = pull
  b = branch
  br = branch -r
  ba = branch -a
  d = diff
  dc = diff --cached
  c = commit
  cm = commit -m
  cp = cherry-pick
  ad = add
  cfe = config -e
  cfeg = config -e --global
  cfes = config -e --system
